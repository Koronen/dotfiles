[alias]
	tree = log --graph --oneline --decorate
	last = show HEAD
	master = checkout master
	uncommit = reset --soft HEAD^
	unstage = reset HEAD

[color]
	branch = auto
	status = auto
	diff = auto

[core]
	excludesfile = ~/.gitignore
	attributesfile = ~/.gitattributes

[diff "exif"]
	textconv = exiftool

[diff "strings"]
	textconv = strings

[github]
	user = Koronen

[user]
	name = Victor Koronen
	email = koronen@kth.se

[apply]
	whitespace = fix
[mergetool]
	keepBackup = false
[difftool]
	prompt = false
[help]
	autocorrect = 1
