[alias]
	master = checkout master
	uncommit = reset --soft HEAD^
	unstage = reset HEAD
	sync = add --all

[apply]
	whitespace = fix

[color]
	ui = auto

[core]
	excludesfile = ~/.gitignore
	attributesfile = ~/.gitattributes

[diff "exif"]
	textconv = exiftool

[diff "strings"]
	textconv = strings

[difftool]
	prompt = false

[fetch]
	prune = true

[github]
	user = Koronen

[help]
	autocorrect = 16

[hub]
	protocol = https

[include]
	path = ~/.gitconfig.local

[init]
	templatedir = ~/.gittemplate

[mergetool]
	keepBackup = false

[push]
	default = current

[user]
	name = Victor Koronen
	email = koronen@kth.se

[web]
	browser = xdg-open
