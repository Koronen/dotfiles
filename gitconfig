[alias]
	tree = log --graph --oneline --decorate
	last = show HEAD
	master = checkout master
	uncommit = reset --soft HEAD^
	unstage = reset HEAD

[apply]
	whitespace = fix

[color]
	branch = auto
	status = auto
	diff = auto

[core]
	excludesfile = ~/.gitignore
	attributesfile = ~/.gitattributes

[diff "exif"]
	textconv = exiftool

[diff "strings"]
	textconv = strings

[difftool]
	prompt = false

[github]
	user = Koronen

[help]
	autocorrect = 1

[mergetool]
	keepBackup = false

[user]
	name = Victor Koronen
	email = koronen@kth.se

