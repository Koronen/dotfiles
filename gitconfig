[alias]
	tree = log --graph  --oneline --decorate
	recent = log --graph  --oneline --decorate -10
	last = log --stat --summary -1
	visual = !gitg
	master = checkout master
	production = checkout production

[core]
	excludesfile = ~/.gitignore

[branch "master"]
	remote = origin
	merge = refs/heads/master
