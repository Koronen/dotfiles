[alias]
	tree = log --graph --oneline --decorate
	recent = log --no-merges --pretty=medium -n 5
	last = show HEAD
	master = checkout master
	production = checkout production

[branch "master"]
	remote = origin
	merge = refs/heads/master

[color]
	branch = auto
	status = auto
	diff = auto

[core]
	excludesfile = ~/.gitignore
	attributesfile = ~/.gitattributes

[diff "exif"]
	textconv = exiftool

[diff "strings"]
	textconv = strings

[github]
	user = Koronen

[user]
	name = Victor Koronen
	email = koronen@kth.se
