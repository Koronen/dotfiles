[color]
	branch = auto
	status = auto
	diff = auto

[alias]
	tree = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	unstage = reset HEAD --
	co = checkout
	br = branch
	ci = commit
	st = status
	last = log -1 HEAD
	recent = log -10 --oneline --graph
	visual = !gitk

[core]
	excludesfile = ~/.gitignore

