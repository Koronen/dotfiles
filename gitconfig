[color]
	branch = auto
	status = auto
	diff = auto

[alias]
	tree = log --graph  --oneline --decorate --abbrev-commit --date=short
	recent = log --graph  --oneline --decorate --abbrev-commit --date=short -10
	last = log --stat --summary -1
	visual = !gitg
	master = checkout master
	production = checkout production

[core]
	excludesfile = ~/.gitignore
